    "contracts/common/SafeHouseNonFungibleTokens.sol": {
      "content": "// SPDX-License-Identifier: MIT\n// Every.finance Contracts\npragma solidity ^0.8.4;\n\nimport \"@openzeppelin/contracts/access/AccessControlEnumerable.sol\";\nimport \"@openzeppelin/contracts/security/Pausable.sol\";\nimport \"@openzeppelin/contracts/token/ERC1155/IERC1155.sol\";\nimport \"@openzeppelin/contracts/token/ERC721/IERC721.sol\";\nimport \"@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol\";\nimport \"@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol\";\n\n/**\n * @dev Implementation of the contract SafeHouseNonFungibleTokens.\n * It allows to manage deposits and withdrawals of Non Fungible Tokens.\n *\n */\n\ncontract SafeHouseNonFungibleTokens is\n\tERC1155Holder,\n\tERC721Holder,\n\tAccessControlEnumerable,\n\tPausable\n{\n\tuint256 public constant DECIMAL_FACTOR = 1e18;\n\tuint256 public maxWithdrawalCapacity;\n\tuint256 public withdrawalCapacity;\n\tuint256 public priceToleranceRate;\n\tbytes32 public constant MANAGER = keccak256(\"MANAGER\");\n\tbytes32 public constant ADMIN = keccak256(\"ADMIN\");\n\tmapping(address => mapping(uint256 => bool)) public tokens;\n\tmapping(address => mapping(uint256 => uint256)) public prices;\n\tevent UpdateMaxWithdrawalCapacity(uint256 maxWithdrawalCapacity_);\n\tevent UpdateWithdrawalCapacity(uint256 withdrawalCapacity_);\n\tevent UpdatePriceToleranceRate(uint256 priceToleranceRate_);\n\tevent UpdateTokenERC721(\n\t\taddress indexed token_,\n\t\tuint256 tokenId_,\n\t\tuint256 price_\n\t);\n\tevent UpdateTokenERC1155(\n\t\taddress indexed token_,\n\t\tuint256 tokenId_,\n\t\tuint256 price_\n\t);\n\tevent RemoveTokenERC721(address indexed token_, uint256 tokenId_);\n\tevent RemoveTokenERC1155(address indexed token_, uint256 tokenId_);\n\n\tevent DepositTokenERC721(\n\t\taddress indexed sender_,\n\t\taddress indexed token_,\n\t\tuint256 tokenId_\n\t);\n\tevent DepositTokenERC1155(\n\t\taddress indexed sender_,\n\t\taddress indexed token_,\n\t\tuint256 tokenId_\n\t);\n\n\tevent WithdrawTokenERC721(\n\t\taddress indexed sender_,\n\t\taddress indexed token_,\n\t\tuint256 tokenId_\n\t);\n\tevent WithdrawTokenERC1155(\n\t\taddress indexed sender_,\n\t\taddress indexed token_,\n\t\tuint256 tokenId_\n\t);\n\n\tconstructor(address admin_, address manager_) {\n\t\trequire(admin_ != address(0), \"Every.finance: zero address\");\n\t\trequire(manager_ != address(0), \"Every.finance: zero address\");\n\t\t_setupRole(DEFAULT_ADMIN_ROLE, admin_);\n\t\t_setupRole(MANAGER, manager_);\n\t}\n\n\t/**\n\t * @dev Update maxWithdrawalCapacity: maximum withrawal capacity.\n\t * @param  maxWithdrawalCapacity_.\n\t * Emits an {UpdateMaxWithdrawalCapacity} event indicating the updated maxWithdrawalCapacity `maxWithdrawalCapacity_`.\n\t */\n\tfunction updateMaxWithdrawalCapacity(\n\t\tuint256 maxWithdrawalCapacity_\n\t) external onlyRole(DEFAULT_ADMIN_ROLE) {\n\t\tmaxWithdrawalCapacity = maxWithdrawalCapacity_;\n\t\temit UpdateMaxWithdrawalCapacity(maxWithdrawalCapacity_);\n\t}\n\n\t/**\n\t * @dev Update withdrawalCapacity: withrawal capacity.\n\t * @param  withdrawalCapacity_.\n\t * Emits an {UpdateWithdrawalCapacity} event indicating the updated withdrawalCapacity `withdrawalCapacity_`.\n\t */\n\tfunction updateWithdrawalCapacity(\n\t\tuint256 withdrawalCapacity_\n\t) external onlyRole(DEFAULT_ADMIN_ROLE) {\n\t\twithdrawalCapacity = withdrawalCapacity_;\n\t\temit UpdateWithdrawalCapacity(withdrawalCapacity_);\n\t}\n\n\t/**\n\t * @dev Update priceToleranceRate: price tolerance to consider its possible fluctuations\n\t * @param  priceToleranceRate_.\n\t * Emits an {UpdatePriceToleranceRate} event indicating the updated priceToleranceRate `priceToleranceRate_`.\n\t */\n\tfunction updatePriceToleranceRate(\n\t\tuint256 priceToleranceRate_\n\t) external onlyRole(DEFAULT_ADMIN_ROLE) {\n\t\trequire(\n\t\t\tpriceToleranceRate_ <= DECIMAL_FACTOR,\n\t\t\t\"Every.finance: out of range\"\n\t\t);\n\t\tpriceToleranceRate = priceToleranceRate_;\n\t\temit UpdatePriceToleranceRate(priceToleranceRate_);\n\t}\n\n\t/**\n\t * @dev update a token.\n\t * a token is a nft ERC721.\n\t * Only the Admin can update a token.\n\t * @param token_ token's ERC721 interface.\n\t * @param tokenId_ token's id.\n\t * @param price_ token's price.\n\t * Emits an {UpdateTokenERC721} event.\n\t */\n\tfunction updateTokenERC721(\n\t\tIERC721 token_,\n\t\tuint256 tokenId_,\n\t\tuint256 price_\n\t) external onlyRole(ADMIN) {\n\t\tif (!tokens[address(token_)][tokenId_]) {\n\t\t\ttokens[address(token_)][tokenId_] = true;\n\t\t}\n\t\tprices[address(token_)][tokenId_] = price_;\n\t\temit UpdateTokenERC721(address(token_), tokenId_, price_);\n\t}\n\n\t/**\n\t * @dev update a token.\n\t * a token is a nft ERC1155.\n\t * Only the Admin can update a token.\n\t * @param token_ token's interface ERC1155.\n\t * @param tokenId_ token's id.\n\t * @param price_ token's price.\n\t * Emits an {UpdateTokenERC1155} event.\n\t */\n\tfunction updateTokenERC1155(\n\t\tIERC1155 token_,\n\t\tuint256 tokenId_,\n\t\tuint256 price_\n\t) external onlyRole(ADMIN) {\n\t\tif (!tokens[address(token_)][tokenId_]) {\n\t\t\ttokens[address(token_)][tokenId_] = true;\n\t\t}\n\t\tprices[address(token_)][tokenId_] = price_;\n\t\temit UpdateTokenERC1155(address(token_), tokenId_, price_);\n\t}\n\n\t/**\n\t * @dev remove a token.\n\t * a token is a nft ERC721.\n\t * Only the Manager can remove a token.\n\t * @param token_ token's ERC721 interface.\n\t * @param tokenId_ token's id.\n\t * Emits an {RemoveTokenERC721} event.\n\t */\n\tfunction removeTokenERC721(\n\t\tIERC721 token_,\n\t\tuint256 tokenId_\n\t) external onlyRole(MANAGER) {\n\t\trequire(tokens[address(token_)][tokenId_], \"Every.finance: no token\");\n\t\ttokens[address(token_)][tokenId_] = false;\n\t\temit RemoveTokenERC721(address(token_), tokenId_);\n\t}\n\n\t/**\n\t * @dev remove a token.\n\t * a token is a nft ERC1155.\n\t * Only the Manager can remove a token.\n\t * @param _token token's ERC1155.\n\t * @param tokenId_ token's id.\n\t * Emits an {RemoveTokenERC1155} event.\n\t */\n\tfunction removeTokenERC1155(\n\t\tIERC721 _token,\n\t\tuint256 tokenId_\n\t) external onlyRole(MANAGER) {\n\t\trequire(tokens[address(_token)][tokenId_], \"Every.finance: no token\");\n\t\ttokens[address(_token)][tokenId_] = false;\n\t\temit RemoveTokenERC1155(address(_token), tokenId_);\n\t}\n\n\t/**\n\t * @dev deposit a ERC721 token in the SafeHouse.\n\t * Only the Manager can deposit a token.\n\t * @param token_ token ERC721 interface.\n\t * @param tokenId_  tokenId.\n\t * Emits an {DepositTokenERC721} event with caller `msg.sender`, token's address  and tokenId `tokenId_`.\n\t */\n\tfunction depositTokenERC721(\n\t\tIERC721 token_,\n\t\tuint256 tokenId_\n\t) external payable whenNotPaused onlyRole(MANAGER) {\n\t\trequire(\n\t\t\ttokens[address(token_)][tokenId_],\n\t\t\t\"Every.finance: no allowed token\"\n\t\t);\n\t\tuint256 withdrawalCapacity_ = withdrawalCapacity;\n\t\twithdrawalCapacity = Math.min(\n\t\t\twithdrawalCapacity_ + prices[address(token_)][tokenId_],\n\t\t\tmaxWithdrawalCapacity\n\t\t);\n\t\ttoken_.safeTransferFrom(msg.sender, address(this), tokenId_);\n\t\temit DepositTokenERC721(msg.sender, address(token_), tokenId_);\n\t}\n\n\t/**\n\t * @dev deposit a ERC1155 token  in the SafeHouse.\n\t * Only the Manager can deposit a token.\n\t * @param token_ token ERC1155 interface.\n\t * @param tokenId_  tokenId.\n\t * Emits an {DepositTokenERC1155} event with caller `msg.sender`, token's address and tokenId `tokenId_`.\n\t */\n\tfunction depositTokenERC1155(\n\t\tIERC1155 token_,\n\t\tuint256 tokenId_\n\t) external payable whenNotPaused onlyRole(MANAGER) {\n\t\trequire(\n\t\t\ttokens[address(token_)][tokenId_],\n\t\t\t\"Every.finance: no allowed token\"\n\t\t);\n\t\tuint256 withdrawalCapacity_ = withdrawalCapacity;\n\t\twithdrawalCapacity = Math.min(\n\t\t\twithdrawalCapacity_ + prices[address(token_)][tokenId_],\n\t\t\tmaxWithdrawalCapacity\n\t\t);\n\t\ttoken_.safeTransferFrom(msg.sender, address(this), tokenId_, 1, \"\");\n\t\temit DepositTokenERC1155(msg.sender, address(token_), tokenId_);\n\t}\n\n\t/**\n\t * @dev withdraw a ERC721 token from the SafeHouse.\n\t * Only the Manager can withdraw a token.\n\t * @param token_ token ERC721 interface.\n\t * @param tokenId_  tokenId.\n\t * Emits an {WithdrawTokenERC721} event with caller `msg.sender`, token's address and tokenId `tokenId_`.\n\t */\n\tfunction withdrawTokenERC721(\n\t\tIERC721 token_,\n\t\tuint256 tokenId_\n\t) external payable whenNotPaused onlyRole(MANAGER) {\n\t\trequire(\n\t\t\ttokens[address(token_)][tokenId_],\n\t\t\t\"Every.finance: no allowed token\"\n\t\t);\n\t\tuint256 price_ = prices[address(token_)][tokenId_];\n\t\trequire(\n\t\t\tMath.min(withdrawalCapacity, maxWithdrawalCapacity) >= price_,\n\t\t\t\"Every.finance: maximum withdrawal amount\"\n\t\t);\n\t\tunchecked {\n\t\t\tprice_ -= Math.mulDiv(price_, priceToleranceRate, DECIMAL_FACTOR);\n\t\t\twithdrawalCapacity -= price_;\n\t\t}\n\t\ttoken_.safeTransferFrom(address(this), msg.sender, tokenId_);\n\t\temit WithdrawTokenERC721(msg.sender, address(token_), tokenId_);\n\t}\n\n\t/**\n\t * @dev withdraw a ERC1155 token from the SafeHouse.\n\t * Only the Manager can withdraw a token.\n\t * @param token_ token ERC1155 interface\n\t * @param tokenId_  tokenId.\n\t * Emits an {WithdrawTokenERC1155} event with caller `msg.sender`, token's address and tokenId `tokenId_`.\n\t */\n\tfunction withdrawTokenERC1155(\n\t\tIERC1155 token_,\n\t\tuint256 tokenId_\n\t) external payable whenNotPaused onlyRole(MANAGER) {\n\t\trequire(\n\t\t\ttokens[address(token_)][tokenId_],\n\t\t\t\"Every.finance: no allowed token\"\n\t\t);\n\t\tuint256 price_ = prices[address(token_)][tokenId_];\n\t\trequire(\n\t\t\tMath.min(withdrawalCapacity, maxWithdrawalCapacity) >= price_,\n\t\t\t\"Every.finance: maximum withdrawal amount\"\n\t\t);\n\t\tunchecked {\n\t\t\tprice_ -= Math.mulDiv(price_, priceToleranceRate, DECIMAL_FACTOR);\n\t\t\twithdrawalCapacity -= price_;\n\t\t}\n\t\ttoken_.safeTransferFrom(address(this), msg.sender, tokenId_, 1, \"\");\n\t\temit WithdrawTokenERC1155(msg.sender, address(token_), tokenId_);\n\t}\n\n\tfunction pause() public onlyRole(DEFAULT_ADMIN_ROLE) {\n\t\t_pause();\n\t}\n\n\tfunction unpause() public onlyRole(DEFAULT_ADMIN_ROLE) {\n\t\t_unpause();\n\t}\n\n\t/// @notice Checks whether the contract supports the given interface.\n\t/// @dev This function checks whether the contract supports the given interface by delegating the call to the inherited contracts ERC1155Receiver and AccessControlEnumerable.\n\t/// @param interfaceId The interface identifier to check.\n\t/// @return A boolean indicating whether the contract supports the given interface.\n\tfunction supportsInterface(\n\t\tbytes4 interfaceId\n\t)\n\t\tpublic\n\t\tview\n\t\toverride(ERC1155Receiver, AccessControlEnumerable)\n\t\treturns (bool)\n\t{\n\t\treturn\n\t\t\tERC1155Receiver.supportsInterface(interfaceId) ||\n\t\t\tAccessControlEnumerable.supportsInterface(interfaceId);\n\t}\n}\n"
    },
    "contracts/gamma/SafeHouseNonFungibleTokensGamma.sol": {
      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.4;\nimport \"../common/SafeHouseNonFungibleTokens.sol\";\n\n/**\n * @author Every.finance.\n * @notice Implementation of the contract SafeHouseNonFungibleTokensGamma.\n */\n\ncontract SafeHouseNonFungibleTokensGamma is SafeHouseNonFungibleTokens {\n\tconstructor(\n\t\taddress admin_,\n\t\taddress manager_\n\t) SafeHouseNonFungibleTokens(admin_, manager_) {}\n}\n"
