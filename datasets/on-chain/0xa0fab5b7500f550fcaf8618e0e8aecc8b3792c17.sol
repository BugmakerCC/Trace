    "Staking.sol": {
      "content": "// SPDX-License-Identifier: MIT\r\npragma solidity ^0.8.20;\r\n\r\nimport \"@openzeppelin/contracts/token/ERC721/IERC721.sol\";\r\nimport \"@openzeppelin/contracts/token/ERC20/IERC20.sol\";\r\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\r\n\r\ncontract EtherWorldStaking is Ownable {\r\n    IERC721 public liquidityNFT;\r\n    IERC20 public rewardToken;\r\n    uint256 public rewardRate;\r\n\r\n    mapping(uint256 => address) public stakers;\r\n    mapping(uint256 => uint256) public stakingTimestamps;\r\n\r\n    event Staked(address indexed user, uint256 tokenId, uint256 timestamp);\r\n    event Unstaked(address indexed user, uint256 tokenId, uint256 reward);\r\n\r\n    constructor(address _liquidityNFT, address _rewardToken, uint256 _rewardRate, address _initialOwner) Ownable(_initialOwner) {\r\n        liquidityNFT = IERC721(_liquidityNFT);\r\n        rewardToken = IERC20(_rewardToken);\r\n        rewardRate = _rewardRate;\r\n    }\r\n\r\n    function stake(uint256 tokenId) external {\r\n        require(liquidityNFT.ownerOf(tokenId) == msg.sender, \"You do not own this NFT\");\r\n        \r\n        liquidityNFT.transferFrom(msg.sender, address(this), tokenId);\r\n        stakers[tokenId] = msg.sender;\r\n        stakingTimestamps[tokenId] = block.timestamp;\r\n\r\n        emit Staked(msg.sender, tokenId, block.timestamp);\r\n    }\r\n\r\n    function unstake(uint256 tokenId) external {\r\n        require(stakers[tokenId] == msg.sender, \"You are not the staker of this NFT\");\r\n\r\n        uint256 stakingDuration = block.timestamp - stakingTimestamps[tokenId];\r\n        uint256 reward = stakingDuration * rewardRate;\r\n\r\n        rewardToken.transfer(msg.sender, reward);\r\n        liquidityNFT.transferFrom(address(this), msg.sender, tokenId);\r\n\r\n        delete stakers[tokenId];\r\n        delete stakingTimestamps[tokenId];\r\n\r\n        emit Unstaked(msg.sender, tokenId, reward);\r\n    }\r\n\r\n    function setRewardRate(uint256 newRewardRate) external onlyOwner {\r\n        rewardRate = newRewardRate;\r\n    }\r\n}\r\n"
    },
