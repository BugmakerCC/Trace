    "Bunnies.sol": {
      "content": "// SPDX-License-Identifier: MIT\r\npragma solidity ^0.8.20;\r\n\r\n/*\r\n        (\\_/)\r\n        (o.o)\r\n        (\")(\")\r\n*/\r\n\r\nimport \"@openzeppelin/contracts/token/ERC721/IERC721.sol\";\r\nimport \"@openzeppelin/contracts/access/Ownable.sol\";\r\nimport \"@openzeppelin/contracts/utils/math/SafeMath.sol\";\r\n\r\ncontract MekaBunniesDeFi is Ownable {\r\n    using SafeMath for uint256;\r\n\r\n    IERC721 public mekaBunniesNFT;\r\n    uint256 public interestRate;\r\n    uint256 public liquidationThreshold;\r\n\r\n    struct Loan {\r\n        uint256 amount;\r\n        uint256 collateralId;\r\n        uint256 startTime;\r\n        bool isActive;\r\n    }\r\n\r\n    mapping(address => Loan) public loans;\r\n\r\n    event LoanTaken(address indexed borrower, uint256 collateralId, uint256 amount);\r\n    event LoanRepaid(address indexed borrower, uint256 collateralId);\r\n    event Liquidated(address indexed borrower, uint256 collateralId);\r\n\r\n    constructor(address _mekaBunniesNFT, uint256 _interestRate, uint256 _liquidationThreshold) Ownable(msg.sender) {\r\n        mekaBunniesNFT = IERC721(_mekaBunniesNFT);\r\n        interestRate = _interestRate;\r\n        liquidationThreshold = _liquidationThreshold;\r\n    }\r\n\r\n    function takeLoan(uint256 _collateralId, uint256 _amount) external {\r\n        require(loans[msg.sender].isActive == false, \"Active loan already exists\");\r\n        require(mekaBunniesNFT.ownerOf(_collateralId) == msg.sender, \"You must own the NFT\");\r\n\r\n        mekaBunniesNFT.transferFrom(msg.sender, address(this), _collateralId);\r\n\r\n        loans[msg.sender] = Loan({\r\n            amount: _amount,\r\n            collateralId: _collateralId,\r\n            startTime: block.timestamp,\r\n            isActive: true\r\n        });\r\n\r\n        payable(msg.sender).transfer(_amount);\r\n        emit LoanTaken(msg.sender, _collateralId, _amount);\r\n    }\r\n\r\n    function repayLoan() external payable {\r\n        Loan memory loan = loans[msg.sender];\r\n        require(loan.isActive, \"No active loan found\");\r\n\r\n        uint256 loanDuration = block.timestamp - loan.startTime;\r\n        uint256 interest = loan.amount.mul(interestRate).mul(loanDuration).div(365 days).div(100);\r\n        uint256 totalRepayment = loan.amount.add(interest);\r\n\r\n        require(msg.value >= totalRepayment, \"Insufficient amount to repay loan\");\r\n\r\n        mekaBunniesNFT.transferFrom(address(this), msg.sender, loan.collateralId);\r\n        loans[msg.sender].isActive = false;\r\n\r\n        if (msg.value > totalRepayment) {\r\n            payable(msg.sender).transfer(msg.value - totalRepayment);\r\n        }\r\n\r\n        emit LoanRepaid(msg.sender, loan.collateralId);\r\n    }\r\n\r\n    function liquidate(address _borrower) external onlyOwner {\r\n        Loan memory loan = loans[_borrower];\r\n        require(loan.isActive, \"No active loan to liquidate\");\r\n\r\n        uint256 loanDuration = block.timestamp - loan.startTime;\r\n        uint256 interest = loan.amount.mul(interestRate).mul(loanDuration).div(365 days).div(100);\r\n        uint256 totalRepayment = loan.amount.add(interest);\r\n\r\n        uint256 collateralValue = loan.amount.mul(150).div(100);\r\n        require(totalRepayment >= collateralValue, \"Loan is not eligible for liquidation\");\r\n\r\n        mekaBunniesNFT.transferFrom(address(this), owner(), loan.collateralId);\r\n        loans[_borrower].isActive = false;\r\n\r\n        emit Liquidated(_borrower, loan.collateralId);\r\n    }\r\n\r\n    receive() external payable {}\r\n}\r\n"
    },
