3._\n     */\n    function functionStaticCall(address target, bytes memory data) internal view returns (bytes memory) {\n        return functionStaticCall(target, data, \"Address: low-level static call failed\");\n    }\n\n    /**\n     * @dev Same as {xref-Address-functionCall-address-bytes-string-}[`functionCall`],\n     * but performing a static call.\n     *\n     * _Available since v3.3._\n     */\n    function functionStaticCall(\n        address target,\n        bytes memory data,\n        string memory errorMessage\n    ) internal view returns (bytes memory) {\n        (bool success, bytes memory returndata) = target.staticcall(data);\n        return verifyCallResultFromTarget(target, success, returndata, errorMessage);\n    }\n\n    /**\n     * @dev Same as {xref-Address-functionCall-address-bytes-}[`functionCall`],\n     * but performing a delegate call.\n     *\n     * _Available since v3.4._\n     */\n    function functionDelegateCall(address target, bytes memory data) internal returns (bytes memory) {\n        return functionDelegateCall(target, data, \"Address: low-level delegate call failed\");\n    }\n\n    /**\n     * @dev Same as {xref-Address-functionCall-address-bytes-string-}[`functionCall`],\n     * but performing a delegate call.\n     *\n     * _Available since v3.4._\n     */\n    function functionDelegateCall(\n        address target,\n        bytes memory data,\n        string memory errorMessage\n    ) internal returns (bytes memory) {\n        (bool success, bytes memory returndata) = target.delegatecall(data);\n        return verifyCallResultFromTarget(target, success, returndata, errorMessage);\n    }\n\n    /**\n     * @dev Tool to verify that a low level call to smart-contract was successful, and revert (either by bubbling\n     * the revert reason or using the provided one) in case of unsuccessful call or if target was not a contract.\n     *\n     * _Available since v4.8._\n     */\n    function verifyCallResultFromTarget(\n        address target,\n        bool success,\n        bytes memory returndata,\n        string memory errorMessage\n    ) internal view returns (bytes memory) {\n        if (success) {\n            if (returndata.length == 0) {\n                // only check isContract if the call was successful and the return data is empty\n                // otherwise we already know that it was a contract\n                require(isContract(target), \"Address: call to non-contract\");\n            }\n            return returndata;\n        } else {\n            _revert(returndata, errorMessage);\n        }\n    }\n\n    /**\n     * @dev Tool to verify that a low level call was successful, and revert if it wasn\u0027t, either by bubbling the\n     * revert reason or using the provided one.\n     *\n     * _Available since v4.3._\n     */\n    function verifyCallResult(\n        bool success,\n        bytes memory returndata,\n        string memory errorMessage\n    ) internal pure returns (bytes memory) {\n        if (success) {\n            return returndata;\n        } else {\n            _revert(returndata, errorMessage);\n        }\n    }\n\n    function _revert(bytes memory returndata, string memory errorMessage) private pure {\n        // Look for revert reason and bubble it up if present\n        if (returndata.length \u003e 0) {\n            // The easiest way to bubble the revert reason is using memory via assembly\n            /// @solidity memory-safe-assembly\n            assembly {\n                let returndata_size := mload(returndata)\n                revert(add(32, returndata), returndata_size)\n            }\n        } else {\n            revert(errorMessage);\n        }\n    }\n}\n"},"ApproveTrade.sol":{"content":"// SPDX-License-Identifier: BSD-3-Clause\npragma solidity ^0.8.0;\n\nimport \"./OwnableUpgradeable.sol\";\nimport \"./ReentrancyGuardUpgradeable.sol\";\nimport { IERC20Upgradeable, SafeERC20Upgradeable } from \"./SafeERC20Upgradeable.sol\";\nimport \"./IERC1271Upgradeable.sol\";\nimport \"./ERC721HolderUpgradeable.sol\";\nimport \"./ECDSAUpgradeable.sol\";\nimport \"./IWETH.sol\";\nimport \"./TransferHelper.sol\";\n\n/**\n * @title Paddlefi\u0027s ApproveTrade Contract\n * @author Paddlefi\n */\ncontract ApproveTrade is OwnableUpgradeable, ERC721HolderUpgradeable, ReentrancyGuardUpgradeable {\n    using SafeERC20Upgradeable for IERC20Upgradeable;\n    using ECDSAUpgradeable for bytes32;\n\n    // Identifier of ERC20\n    address private constant ERC20 = 0x0000000000000000000000000000000000000020;\n\n    // Identifier of ERC721\n    address private constant ERC721 = 0x0000000000000000000000000000000000000721;\n\n    // Denominator, used for calculating percentage\n    uint256 private constant DENOMINATOR = 10000;\n\n    // keccak256(\"Asset(address collection,address assetClass,uint256 amountOrID)\")\n    bytes32 public ASSET_HASH;\n\n    // keccak256(\"Order(address maker,address taker,address collection[],address assetClass[],uint256[] amountOrID,address currency,uint256 price,uint256 deadline,uint256 duration,uint256 interestPerSecond,uint256 nonce)\")\n    bytes32 public ORDER_HASH;\n\n    // use for EIP712 \n    bytes32 public DOMAIN_SEPARATOR;\n\n    /// @notice WETH contract address\n    address public WETH;\n\n    /// @notice Platform fee rate\n    uint256 public feeRate;\n\n    /// @notice Fee receiver\n    address payable public feeTo;\n\n    /// @notice ERC721 transfer manager address\n    address public nftTransferManager;\n\n    /**\n     * @notice Asset info\n     * @member collection collection address list\n     * @member assetClass asset class (e.g., ERC20,ERC721)\n     * @member amountOrID amount of tokens to sell/purchase (must be 1 for ERC721, 1+ for other)\n     */\n    struct Asset{\n        address collection;\n        address assetClass;\n        uint256 amountOrID;\n    }\n    /**\n     * @notice Order info\n     * @member maker maker of the order\n     * @member taker taker of the order(0: all can trade)\n     * @member asset asset list\n     * @member currency currency (e.g., WETH)\n     * @member price price (used for sell)\n     * @member deadline deadline in timestamp - 0 for no expiry\n     * @member duration\n     * @member interestPerSecond\n     * @member endTime\n     * @member sig Signature\n     */\n    struct Order {\n        address maker;\n        address taker;\n        Asset[] asset;\n        address currency;\n        uint256 price;\n        uint256 deadline;\n        uint256 duration;\n        uint256 interestPerSecond;\n        uint256 endTime;\n        bytes sig;\n    }\n\n    /// @notice Record the nonce for the address signature handling listings and offers\n    mapping(address =\u003e uint256) public nonces;\n\n    /// @notice Identifier of order cancellation or success\n    mapping(bytes32 =\u003e bool) public cancelledOrFinalized;\n\n    /// @notice all loan order\n    mapping(bytes32 =\u003e Order) public loanOrders;\n\n    /// @notice min interest rate\n    uint256 public minInterestRate;\n\n    /// @notice interest fee rate\n    uint256 public interestFeeRate;\n\n    /// @notice Emitted when nonce increases\n    event CancelAllOrders(address maker, uint256 nonce);\n\n    /// @notice Emitted when cancel multiple orders\n    event CancelMultipleOrders(address maker,bytes32[] hashs);\n\n    /// @notice Emitted when cancel order\n    event CancelOrders(address maker,bytes32 hash);\n\n    /// @notice Emitted when offer is taken\n    event Loan(address lender, address borrower, bytes32 orderHash, uint256 fee,uint256 endTime);\n\n    /// @notice Emitted when offer is taken\n    event Redeem(address lender, address borrower, bytes32 orderHash, uint256 fee,uint256 interest);\n\n    /// @notice Emitted when offer is taken\n    event Liquidate(address lender, address borrower, bytes32 orderHash);\n\n    /// @notice Emitted when offer is taken\n    event Buy(address buyer, address seller, bytes32 orderHash, uint256 fee);\n\n    /// @notice Emitted when listing is matched\n    event Sell(address buyer, address seller, bytes32 orderHash, uint256 fee);\n\n    /**\n     * @notice Initialize contract parameters - only execute once\n     * @param owner_ Owner address\n     * @param WETH_ WETH address\n     * @param feeTo_ Fee receiver\n     * @param nftTransferManager_ ERC721 transfer manager address\n     * @param feeRate_ Platform fee rate\n     * @param minInterestRate_ order min interest rate\n     * @param interestFeeRate_ order interest fee rate\n     */\n    function initialize(address owner_,address WETH_,address nftTransferManager_,address payable feeTo_,uint256 feeRate_,uint256 minInterestRate_,uint256 interestFeeRate_) external initializer {\n        _transferOwnership(owner_);\n        __ReentrancyGuard_init();\n        WETH = WETH_;\n        feeTo = feeTo_;\n        nftTransferManager = nftTransferManager_;\n        feeRate = feeRate_;\n        minInterestRate = minInterestRate_;\n        interestFeeRate = interestFeeRate_;\n\n        ASSET_HASH = keccak256(\"Asset(address collection,address assetClass,uint256 amountOrID)\");\n\n        ORDER_HASH = keccak256(\"Order(address maker,address taker,Asset[] asset,address currency,uint256 price,uint256 deadline,uint256 duration,uint256 interestPerSecond,uint256 nonce)Asset(address collection,address assetClass,uint256 amountOrID)\");\n\n        uint256 chainId;\n        assembly {\n            chainId := chainid()\n        }\n\n        DOMAIN_SEPARATOR = keccak256(\n            abi.encode(\n                keccak256(\"EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)\"),\n                keccak256(bytes(\"ApproveTrade\")),\n                keccak256(bytes(\"1\")),\n                chainId,\n                address(this)\n            )\n        );\n    }\n\n    receive() external payable {}\n\n    /**\n     * @notice update\n     * @param nftTransferManager_ ERC721 transfer manager address\n     * @param feeTo_ Fee receiver\n     * @param feeRate_ Platform fee rate\n     * @param minInterestRate_ order min interest rate \n     * @param interestFeeRate_ order interest fee rate\n     */\n    function update(address nftTransferManager_,address payable feeTo_,uint256 feeRate_,uint256 minInterestRate_,uint256 interestFeeRate_) external onlyOwner {\n        nftTransferManager = nftTransferManager_;\n        feeTo = feeTo_;\n        feeRate = feeRate_;\n        minInterestRate = minInterestRate_;\n        interestFeeRate = interestFeeRate_;        \n    }\n\n    /**\n     * @notice Increase nonce，cancel all listings and offers\n     */\n    function cancelAllOrders() external {\n        uint256 newNonce = ++nonces[msg.sender];\n        emit CancelAllOrders(msg.sender, newNonce);\n    }\n\n    /**\n     * @notice Cancel multiple orders\n     * @param orders Order array\n     */\n\tfunction cancelMultipleOrders(Order[] calldata orders) external {\n        bytes32[] memory hashs = new bytes32[](orders.length);\n\t\tfor(uint i = 0; i \u003c orders.length; i++) {\n\t\t\thashs[i] = _cancelOrder(orders[i]);\n\t\t}\n        emit CancelMultipleOrders(msg.sender,hashs);\n\t}\n\n    /**\n     * @notice Cancel single order\n     * @param order Order\n     */\n    function cancelOrder(Order calldata order) public {\n        bytes32 orderHash = _cancelOrder(order);\n        emit CancelOrders(msg.sender,orderHash);\n    }\n\n    /**\n     * @notice Cancel single order\n     * @param order order\n     * @return bytes32 order hash\n     */\n    function _cancelOrder(Order calldata order) private returns (bytes32) {\n        require(msg.sender == order.maker, \"maker\");\n        bytes32 orderHash = hashOrder(order);\n        require(!cancelledOrFinalized[orderHash], \"status\");\n        cancelledOrFinalized[orderHash] = true;\n        return orderHash;\n    }\n\n    /**\n     * @notice lender confirms the order by checking borrower\u0027s signature\n     * @param loanOrder loan order info\n     */\n    function loan(Order calldata loanOrder) public payable nonReentrant {\n        require(loanOrder.taker == address(0) || loanOrder.taker == msg.sender,\"taker\");\n        require(loanOrder.duration \u003e 0 \u0026\u0026 loanOrder.interestPerSecond \u003e 0,\"interest\");\n        bytes32 orderHash = _validate(loanOrder);\n        cancelledOrFinalized[orderHash] = true;\n        loanOrders[orderHash] = loanOrder;\n        loanOrders[orderHash].taker = msg.sender;\n        loanOrders[orderHash].endTime = loanOrder.duration + block.timestamp;\n        _swapAssets(loanOrder,loanOrder.maker,address(this));\n        uint256 fee = _calcTradeFee(loanOrder.price);\n        _swapCurrency(loanOrder,msg.sender, loanOrder.maker,0,fee);\n        emit Loan(msg.sender, loanOrder.maker, orderHash, fee,loanOrders[orderHash].endTime);\n    }\n\n    /**\n     * @notice borrower redeem assets by pay interest\n     * @param orderHash loan order info hash\n     */\n    function redeem(bytes32 orderHash) public payable nonReentrant {\n        Order storage loanOrder = loanOrders[orderHash];\n        require(loanOrder.maker == msg.sender,\"maker\");\n        require(block.timestamp \u003e loanOrder.endTime - loanOrder.duration,\"startTime\");\n        require(loanOrder.endTime \u003e= block.timestamp,\"endTime\");\n        uint256 interest = _calcInterest(loanOrder.duration,loanOrder.endTime - loanOrder.duration,loanOrder.interestPerSecond);\n        uint256 fee = _calcInterestFee(interest);\n        loanOrder.endTime = 0;\n        _swapCurrency(loanOrder,msg.sender, loanOrder.taker,interest,fee);\n        _swapAssets(loanOrder,address(this),loanOrder.maker);\n        emit Redeem(loanOrder.taker, msg.sender, orderHash, fee,interest);\n    }\n\n    /**\n     * @notice lender liquidate assets when borrow to end time\n     * @param orderHash loan order info\n     */\n    function liquidate(bytes32 orderHash) public nonReentrant {\n        Order storage loanOrder = loanOrders[orderHash];\n        require(loanOrder.taker == msg.sender,\"taker\");\n        require(loanOrder.endTime \u003e 0 \u0026\u0026 loanOrder.endTime \u003c block.timestamp,\"endTime\");\n        loanOrder.endTime = 0;\n        _swapAssets(loanOrder,address(this),loanOrder.taker);\n        emit Liquidate(loanOrder.taker, loanOrder.maker, orderHash);\n    }\n\n    /**\n     * @notice calc order interst\n     * @param duration max time when loan amount\n     * @param startTime start time when loan amount\n     * @param interestPerSecond interest per second\n     * @return interest order interst\n     */\n    function _calcInterest(uint256 duration,uint256 startTime,uint256 interestPerSecond) private view returns (uint256 interest) {\n        uint256 minInterest = duration * interestPerSecond * minInterestRate / DENOMINATOR /1e18;\n        interest = (block.timestamp - startTime) * interestPerSecond/1e18;\n        if(interest \u003c minInterest){\n            interest = minInterest;\n        }\n    }\n\n    /**\n     * @notice calc platform trade fee\n     * @param amount trade amount or interest amount\n     * @return fee platform fee\n     */\n    function _calcTradeFee(uint256 amount) private view returns (uint256 fee) {\n        fee = amount * feeRate / DENOMINATOR;\n    }\n\n    /**\n     * @notice calc platform interest fee\n     * @param amount laon interest\n     * @return fee platform fee\n     */\n    function _calcInterestFee(uint256 amount) private view returns (uint256 fee) {\n        fee = amount * interestFeeRate / DENOMINATOR;\n    }\n\n    /**\n     * @notice Buyer confirms the order by checking seller\u0027s signature\n     * @param sellOrder Seller order info\n     */\n    function buy(Order calldata sellOrder) public payable nonReentrant {\n        require(sellOrder.taker == address(0) || sellOrder.taker == msg.sender,\"taker\");\n        require(sellOrder.duration == 0 \u0026\u0026 sellOrder.interestPerSecond == 0,\"amount\");\n        bytes32 orderHash = _validate(sellOrder);\n        cancelledOrFinalized[orderHash] = true;\n        _swapAssets(sellOrder,sellOrder.maker,msg.sender);\n        uint256 fee = _calcTradeFee(sellOrder.price);\n        _swapCurrency(sellOrder,msg.sender,sellOrder.maker,0,fee);\n        emit Buy(msg.sender, sellOrder.maker, orderHash, fee);\n    }\n\n    /**\n     * @notice Seller confirms the order by checking buyer\u0027s signature\n     * @param buyOrder Buyer order info\n     */\n    function sell(Order calldata buyOrder) public nonReentrant {\n        require(buyOrder.taker == address(0) || buyOrder.taker == msg.sender,\"taker\");\n        require(buyOrder.duration == 0 \u0026\u0026 buyOrder.interestPerSecond == 0,\"amount\");\n        bytes32 orderHash = _validate(buyOrder);\n        cancelledOrFinalized[orderHash] = true;\n        _swapAssets(buyOrder,msg.sender,buyOrder.maker);\n        uint256 fee = _calcTradeFee(buyOrder.price);\n        _swapCurrency(buyOrder,buyOrder.maker,msg.sender,0,fee);\n        emit Sell(buyOrder.maker, msg.sender, orderHash, fee);\n    }\n\n    /**\n     * @notice Exchange Currency\n     * @param order Order info\n     * @param from Buyer\n     * @param to Seller\n     * @param extraValue extra amount\n     * @param fee PLatform fee\n     */\n    function _swapCurrency(Order memory order,address from, address to, uint256 extraValue,uint256 fee) private {\n        uint256 totalValue = order.price + extraValue;\n        if (order.currency == address(1)) {\n            require(msg.value \u003e 0 \u0026\u0026 msg.value \u003e= totalValue, \"value\");\n            if(msg.value \u003e totalValue){\n                _transferCurrency(order.currency, from, msg.value - totalValue);\n            }\n        }else {\n            IERC20Upgradeable(order.currency).safeTransferFrom(from, address(this), totalValue);\n        }\n        _transferCurrency(order.currency, feeTo, fee);\n        _transferCurrency(order.currency, to, totalValue - fee);\n    }\n\n\n    /**\n     * @notice Exchange assets\n     * @param order Order info\n     * @param from Buyer\n     * @param to Seller\n     */\n    function _swapAssets(Order memory order,address from, address to) private {\n        for(uint i=0;i\u003corder.asset.length;i++){\n            require(order.asset[i].assetClass == ERC20 || order.asset[i].assetClass == ERC721, \"assetClass\");\n            if(order.asset[i].assetClass == ERC721){\n                if(from == address(this)){\n                    TransferHelper.transferOutNonFungibleToken(nftTransferManager, order.asset[i].collection, from, to, order.asset[i].amountOrID);\n                }else{\n                    TransferHelper.transferInNonFungibleToken(nftTransferManager, order.asset[i].collection, from, to, order.asset[i].amountOrID);\n                }\n            }else if(order.asset[i].assetClass == ERC20){\n                if(from == address(this)){\n                    IERC20Upgradeable(order.asset[i].collection).safeTransfer(to, order.asset[i].amountOrID);\n                }else{\n                    IERC20Upgradeable(order.asset[i].collection).safeTransferFrom(from, to, order.asset[i].amountOrID);\n                }\n            }\n        }\n    }\n\n    /**\n     * @notice Send ERC20 asset\n     * @param token token address\n     * @param recipient Receiver\n     * @param amount Sent amount\n     */\n    function _transferCurrency(address token, address recipient, uint256 amount) private {\n        if(recipient != address(0) \u0026\u0026 amount\u003e0){\n            if(token == address(1)) {\n                payable(recipient).transfer(amount);\n            } else {\n                IERC20Upgradeable(token).safeTransfer(recipient, amount);\n            }\n        }\n    }\n\n    function _hashAsset(Asset calldata asset) internal view returns (bytes32) {\n        return keccak256(abi.encode(ASSET_HASH, asset.collection, asset.assetClass, asset.amountOrID));\n    }\n    /**\n     * @notice Get hash of order information\n     * @param assets Order info\n     * @return assetsHash order hash\n     */\n    function _packAsset(Asset[] calldata assets) internal view returns (bytes32) {\n        bytes32[] memory assetHashes = new bytes32[](assets.length);\n        uint256 assetsLength = assets.length;\n        for (uint256 i; i \u003c assetsLength; ) {\n            assetHashes[i] = _hashAsset(assets[i]);\n            unchecked {\n                ++i;\n            }\n        }\n        return keccak256(abi.encodePacked(assetHashes));\n    }\n\n    /**\n     * @notice Get hash of order information\n     * @param order Order info\n     * @return orderHash order hash\n     */\n    function hashOrder(Order calldata order) public view returns (bytes32 orderHash) {\n        bytes32 structHash = keccak256(abi.encode(\n            ORDER_HASH,\n            order.maker,\n            order.taker,\n            _packAsset(order.asset),\n            order.currency,\n            order.price,\n            order.deadline,\n            order.duration,\n            order.interestPerSecond,\n            nonces[order.maker]\n        ));\n        orderHash = keccak256( abi.encodePacked(\"\\x19\\x01\", DOMAIN_SEPARATOR, structHash) );\n    }\n\n    /**\n     * @notice Signature validation\n     * @param order Order info\n     * @return digest Order info hash\n     */\n    function _validate(Order calldata order) internal view returns (bytes32 digest) {\n        require(order.maker != msg.sender,\"maker\");\n        require(order.asset.length \u003e 0, \"size\");\n        require(order.deadline \u003e= block.timestamp, \"deadline\");\n        require(order.price \u003e 0, \"price\");\n\n        digest = hashOrder(order);\n        require(!cancelledOrFinalized[digest], \"status\");\n\n        if (AddressUpgradeable.isContract(order.maker)) {\n            // 0x1626ba7e is the interfaceId for signature contracts (see IERC1271)\n            require(IERC1271Upgradeable(order.maker).isValidSignature(digest, order.sig) == 0x16